if (CMAKE_VERSION VERSION_LESS 3.13.0)
    message(FATAL_ERROR "@BASYX_UTIL_LIBRARY_NAME@ needs at least CMake version 3.13.0")
endif()

get_filename_component(_basyx_util_rootdir ${CMAKE_CURRENT_LIST_DIR}/../../../ ABSOLUTE)

set(BASYX_UTIL_VERSION_STRING "@PROJECT_VERSION@")

set(BASYX_UTIL_INSTALL_PREFIX @CMAKE_INSTALL_PREFIX@)
set(BASYX_UTIL_INSTALL_LIBDIR ${BASYX_UTIL_INSTALL_PREFIX}/@CMAKE_INSTALL_LIBDIR@)
set(BASYX_UTIL_INSTALL_INCLUDE_DIR ${BASYX_UTIL_INSTALL_PREFIX}/include)
set(BASYX_UTIL_INSTALL_INCLUDE_DIR_BASYX_UTIL ${BASYX_UTIL_INSTALL_PREFIX}/include/@PROJECT_SHORTNAME@)
set(BASYX_UTIL_INCLUDE_DIRS   ${_basyx_util_rootdir}${BASYX_UTIL_INSTALL_INCLUDE_DIR} ${_basyx_util_rootdir}${BASYX_UTIL_INSTALL_INCLUDE_DIR_BASYX_UTIL})
set(BASYX_UTIL_LIBRARY_DIR    ${_basyx_util_rootdir}${BASYX_UTIL_INSTALL_LIBDIR})
set(BASYX_UTIL_LIBRARIES      -L${BASYX_UTIL_LIBRARY_DIR} -l@BASYX_UTIL_LIBRARY_NAME@)

list(APPEND BASYX_UTIL_LIBRARIES)

mark_as_advanced( BASYX_UTIL_INCLUDE_DIRS BASYX_UTIL_LIBRARIES )
