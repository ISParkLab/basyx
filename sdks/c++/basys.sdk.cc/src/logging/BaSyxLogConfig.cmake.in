if (CMAKE_VERSION VERSION_LESS 3.13.0)
    message(FATAL_ERROR "@BASYX_LOG_LIBRARY_NAME@ needs at least CMake version 3.13.0")
endif()

get_filename_component(_basyx_log_rootdir ${CMAKE_CURRENT_LIST_DIR}/../../../ ABSOLUTE)

set(BASYX_LOG_VERSION_STRING "@PROJECT_VERSION@")

set(BASYX_LOG_INSTALL_PREFIX @CMAKE_INSTALL_PREFIX@)
set(BASYX_LOG_INSTALL_LIBDIR ${BASYX_LOG_INSTALL_PREFIX}/@CMAKE_INSTALL_LIBDIR@)
set(BASYX_LOG_INSTALL_INCLUDE_DIR ${BASYX_LOG_INSTALL_PREFIX}/include)
set(BASYX_LOG_INSTALL_INCLUDE_DIR_BASYX_LOG ${BASYX_LOG_INSTALL_PREFIX}/include/@PROJECT_SHORTNAME@)
set(BASYX_LOG_INCLUDE_DIRS   ${_basyx_log_rootdir}${BASYX_LOG_INSTALL_INCLUDE_DIR} ${_basyx_log_rootdir}${BASYX_LOG_INSTALL_INCLUDE_DIR_BASYX_LOG})
set(BASYX_LOG_LIBRARY_DIR    ${_basyx_log_rootdir}${BASYX_LOG_INSTALL_LIBDIR})
set(BASYX_LOG_LIBRARIES      -L${BASYX_LOG_LIBRARY_DIR} -l@BASYX_LOG_LIBRARY_NAME@)

list(APPEND BASYX_LOG_LIBRARIES)

mark_as_advanced( BASYX_LOG_INCLUDE_DIRS BASYX_LOG_LIBRARIES )
