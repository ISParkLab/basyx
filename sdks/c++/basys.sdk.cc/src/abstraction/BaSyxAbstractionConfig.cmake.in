if (CMAKE_VERSION VERSION_LESS 3.13.0)
    message(FATAL_ERROR "@BASYX_ABSTRACTION_LIBRARY_NAME@ needs at least CMake version 3.13.0")
endif()

get_filename_component(_basyx_abstraction_rootdir ${CMAKE_CURRENT_LIST_DIR}/../../../ ABSOLUTE)

set(BASYX_ABSTRACTION_VERSION_STRING "@PROJECT_VERSION@")

set(BASYX_ABSTRACTION_INSTALL_PREFIX @CMAKE_INSTALL_PREFIX@)
set(BASYX_ABSTRACTION_INSTALL_LIBDIR ${BASYX_ABSTRACTION_INSTALL_PREFIX}/@CMAKE_INSTALL_LIBDIR@)
set(BASYX_ABSTRACTION_INSTALL_INCLUDE_DIR ${BASYX_ABSTRACTION_INSTALL_PREFIX}/include)
set(BASYX_ABSTRACTION_INSTALL_INCLUDE_DIR_BASYX_ABSTRACTION ${BASYX_ABSTRACTION_INSTALL_PREFIX}/include/@PROJECT_SHORTNAME@)
set(BASYX_ABSTRACTION_INCLUDE_DIRS   ${_basyx_abstraction_rootdir}${BASYX_ABSTRACTION_INSTALL_INCLUDE_DIR} ${_basyx_abstraction_rootdir}${BASYX_ABSTRACTION_INSTALL_INCLUDE_DIR_BASYX_ABSTRACTION})
set(BASYX_ABSTRACTION_LIBRARY_DIR    ${_basyx_abstraction_rootdir}${BASYX_ABSTRACTION_INSTALL_LIBDIR})
set(BASYX_ABSTRACTION_LIBRARIES      -L${BASYX_ABSTRACTION_LIBRARY_DIR} -l@BASYX_ABSTRACTION_LIBRARY_NAME@)

list(APPEND BASYX_ABSTRACTION_LIBRARIES)

mark_as_advanced( BASYX_ABSTRACTION_INCLUDE_DIRS BASYX_ABSTRACTION_LIBRARIES )
