if (CMAKE_VERSION VERSION_LESS 3.13.0)
    message(FATAL_ERROR "@BASYX_VAB_LIBRARY_NAME@ needs at least CMake version 3.13.0")
endif()

get_filename_component(_basyx_vab_rootdir ${CMAKE_CURRENT_LIST_DIR}/../../../ ABSOLUTE)

set(BASYX_VAB_VERSION_STRING "@PROJECT_VERSION@")

set(BASYX_VAB_INSTALL_PREFIX @CMAKE_INSTALL_PREFIX@)
set(BASYX_VAB_INSTALL_LIBDIR ${BASYX_VAB_INSTALL_PREFIX}/@CMAKE_INSTALL_LIBDIR@)
set(BASYX_VAB_INSTALL_INCLUDE_DIR ${BASYX_VAB_INSTALL_PREFIX}/include)
set(BASYX_VAB_INSTALL_INCLUDE_DIR_BASYX_VAB ${BASYX_VAB_INSTALL_PREFIX}/include/@PROJECT_SHORTNAME@)
set(BASYX_VAB_INCLUDE_DIRS   ${_basyx_vab_rootdir}${BASYX_VAB_INSTALL_INCLUDE_DIR} ${_basyx_vab_rootdir}${BASYX_VAB_INSTALL_INCLUDE_DIR_BASYX_VAB})
set(BASYX_VAB_LIBRARY_DIR    ${_basyx_vab_rootdir}${BASYX_VAB_INSTALL_LIBDIR})
set(BASYX_VAB_LIBRARIES      -L${BASYX_VAB_LIBRARY_DIR} -l@BASYX_VAB_LIBRARY_NAME@)

list(APPEND BASYX_VAB_LIBRARIES)

mark_as_advanced( BASYX_VAB_INCLUDE_DIRS BASYX_VAB_LIBRARIES )
