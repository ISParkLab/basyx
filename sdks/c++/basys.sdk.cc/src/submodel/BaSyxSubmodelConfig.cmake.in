if (CMAKE_VERSION VERSION_LESS 3.13.0)
    message(FATAL_ERROR "@BASYX_Submodel_LIBRARY_NAME@ needs at least CMake version 3.13.0")
endif()

get_filename_component(_basyx_submodel_rootdir ${CMAKE_CURRENT_LIST_DIR}/../../../ ABSOLUTE)

set(BASYX_Submodel_VERSION_STRING "@PROJECT_VERSION@")

set(BASYX_Submodel_INSTALL_PREFIX @CMAKE_INSTALL_PREFIX@)
set(BASYX_Submodel_INSTALL_LIBDIR ${BASYX_Submodel_INSTALL_PREFIX}/@CMAKE_INSTALL_LIBDIR@)
set(BASYX_Submodel_INSTALL_INCLUDE_DIR ${BASYX_Submodel_INSTALL_PREFIX}/include)
set(BASYX_Submodel_INSTALL_INCLUDE_DIR_BASYX_Submodel ${BASYX_Submodel_INSTALL_PREFIX}/include/@PROJECT_SHORTNAME@)
set(BASYX_Submodel_INCLUDE_DIRS   ${_basyx_submodel_rootdir}${BASYX_Submodel_INSTALL_INCLUDE_DIR} ${_basyx_submodel_rootdir}${BASYX_Submodel_INSTALL_INCLUDE_DIR_BASYX_Submodel})
set(BASYX_Submodel_LIBRARY_DIR    ${_basyx_submodel_rootdir}${BASYX_Submodel_INSTALL_LIBDIR})
set(BASYX_Submodel_LIBRARIES      -L${BASYX_Submodel_LIBRARY_DIR} -l@BASYX_Submodel_LIBRARY_NAME@)

list(APPEND BASYX_Submodel_LIBRARIES)

mark_as_advanced( BASYX_Submodel_INCLUDE_DIRS BASYX_Submodel_LIBRARIES )
